{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "htmljinja",
      "camelcase": "htmlJinja",
      "title": "Htmljinja",
      "scope": "source.htmljinja",
      "file-types": [
        "html"
      ],
      "highlights": "queries/htmljinja/highlights.scm",
      "injections": "queries/htmljinja/injections.scm",
      "injection-regex": "^htmljinja$",
      "class-name": "TreeSitterHtmljinja"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "A tree-sitter grammar for the Django template language (Slightly modified to work better with Jinja2)",
    "authors": [
      {
        "name": "William Vandervalk",
        "url": "https://williamvandervalk.com/"
      },
      {
        "name": "Ahmad Ansori Palembani",
        "url": "https://aap.my.id/"
      }
    ],
    "links": {
      "repository": "https://github.com/null2264/tree-sitter-htmldjango"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}